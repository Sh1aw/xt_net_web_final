@using Epam.ExtPosterStore.BLL.Ioc
@using Epam.ExtPosterStore.Entities
@using Epam.ExtPosterStore.WebPagesPL.Common
@{
    if (IsPost)
    {
        if (InputNormalizer.ValidateInt(Request.Form["itemId"]))
        {
            var typeLogic = DependencyResolver.TypeOfProductLogic;
            var typeTittle = Request.Form["itemTittle"];
            var typeId = Int32.Parse(Request.Form["itemId"]);
            Response.StatusCode = typeLogic.Update(typeTittle, typeId);
        }

    }
}
